{
  "name": "gulp-starter",
  "version": "0.1.1",
  "description": "Gulp starter",
  "repository": {
    "type": "git",
    "url": "git://github.com/owlookit/owl-gulp.git"
  },
  "browser": {
    "underscore": "backbone/node_modules/underscore",
    "plugin": "./src/scripts/lib/jquery-plugin.js",
    "clock": "./src/scripts/clock.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify",
      "hbsfy"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    },
    "clock": {
      "exports": "clock"
    }    
  },
  "devDependencies": {
    "browser-sync": "~1.3.6",
    "browserify": "~5.10.1",
    "browserify-shim": "~3.7.0",
    "coffeeify": "~0.7.0",
    "gulp": "^3.8.7",
    "gulp-autoprefixer": "^2.0.0",
    "gulp-changed": "^0.4.1",
    "gulp-imagemin": "^0.6.2",
    "gulp-notify": "^1.4.2",
    "gulp-ruby-sass": "^0.7.1",
    "gulp-sass": "^1.1.0",
    "gulp-sourcemaps": "^1.2.8",
    "gulp-util": "^3.0.0",
    "handlebars": "^1.3.0",
    "hbsfy": "~2.0.0",
    "pretty-hrtime": "~0.2.1",
    "require-dir": "^0.1.0",
    "vinyl-source-stream": "~0.1.1",
    "watchify": "~1.0.2",
    "gulp-jade": "*",
    "gulp-kss": "0.0.2",
    "gulp-plumber": "*"
  },
  "dependencies": {
    "backbone": "~1.1.2",
    "jquery": "~2.1.0"
  }
}
